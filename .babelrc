{
  "presets": ["stage-0", "react"],
  "plugins": [
    [
      "transform-es2015-modules-commonjs",
      "transform-async-to-module-method",
      "react-transform",
      {
        "transforms": [
          {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"],
          }
        ]
      }
    ]
  ],
  "ignore": ["build/server.js"]
}
