{
  "name": "financebutler",
  "version": "1.0.0",
  "description": "simple finance managment system",
  "main": "start.js",
  "scripts": {
    "build": "cross-env BUILD=1 NODE_ENV=production DEBUG=process* node start.js",
    "start": "cross-env NODE_ENV=production DEBUG=process*,app:* node start.js",
    "test": "npm run test:api && npm run test:e2e",
    "test:api": "cross-env TEST=1 ava test/api/*.js --tap | tap-summary",
    "test:e2e": "cross-env E2E=1 NODE_ENV=production DEBUG=process*,app:error node start.js",
    "dev": "cross-env BUILD=1 NODE_ENV=development DEBUG=process*,app:* node start.js",
    "pack": "nar create"
  },
  "keywords": [
    "money"
  ],
  "ava": {
    "source": [
      "src/*.js",
      "!build/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "archive": {
    "dependencies": true,
    "devDependencies": false,
    "patterns": [
      "build/**"
    ]
  },
  "author": "Boris Ryabov",
  "license": "GPL-3.0",
