{
  "name": "financebutler",
  "version": "1.0.0",
  "description": "simple finance managment system",
  "main": "start.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development DEBUG=app:* node start.js",
    "prod": "cross-env NODE_ENV=production DEBUG=app:* node start.js",
    "test": "cross-env TEST=1 NODE_ENV=production node start.js && ava --tap | faucet",
    "test:watch": "cross-env TEST=1 NODE_ENV=production node start.js && ava --watch --tap | faucet",
    "pack": "nar create"
  },
  "keywords": [
    "money"
  ],
  "ava": {
    "files": [
      "test/api/*.js"
    ],
    "source": [
      "src/*.js",
      "!build/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "archive": {
    "dependencies": true,
    "devDependencies": false,
    "patterns": [
      "build/**"
    ]
  },
  "author": "Boris Ryabov",
  "license": "GPL-3.0",
  "dependencies": {
