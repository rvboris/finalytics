{
  "name": "finalytics",
  "version": "1.0.0",
  "description": "Simple finance managment system",
  "main": "build/server/main.js",
  "scripts": {
    "clean": "rimraf build",
    "dev": "node ./dev",
    "build": "yarn run clean && webpack --env.mode production --config webpack/client-config.js && webpack --env.mode production --config webpack/server-config.js",
    "start": "cross-env DEBUG=process*,app:*,-app:db node build/server/main.js",
    "test": "yarn run test:api && yarn run test:e2e",
    "test:api": "cross-env TEST=1 DEBUG=process*,app:error ava test/api/*.js",
    "test:e2e": "cross-env E2E=1 DEBUG=process*,app:error node test/e2e.js"
  },
  "ava": {
    "source": [
      "src/*.js",
      "!build/**/*"
    ],
    "failFast": true,
    "tap": false,
    "concurrency": 4,
    "powerAssert": true,
    "require": [
      "babel-register"
    ]
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "private": true,
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ],
    "ignore": [
      "build/server/main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvboris/finalytics.git"
  },
  "keywords": [
    "money",
    "finance",
    "budget"
  ],
  "author": "Boris Ryabov <rv.boris@yandex.ru>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rvboris/finalytics/issues"
  },
