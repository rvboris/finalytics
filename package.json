{
  "name": "financebutler",
  "version": "0.0.1",
  "description": "Simple finance managment system",
  "main": "build/server/main.js",
  "scripts": {
    "clean": "rimraf build",
    "development": "node ./dev",
    "build": "npm run clean && webpack --env.mode production --config webpack/client-config.js && webpack --env.mode production --config webpack/server-config.js",
    "start": "cross-env DEBUG=process*,app:*,-app:db node build/server/main.js",
    "test": "npm run test:api && npm run test:e2e",
    "test:api": "cross-env TEST=1 DEBUG=process*,app:error ava test/api/*.js --tap | tap-summary",
    "test:e2e": "cross-env E2E=1 DEBUG=process*,app:error node test/e2e.js"
  },
  "ava": {
    "source": [
      "src/*.js",
      "!build/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "stage-1"
    ],
    "ignore": [
      "build/server/main"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvboris/financebutler.git"
  },
  "keywords": [
    "money"
  ],
  "author": "Boris Ryabov <rv.boris@yandex.ru>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/rvboris/financebutler/issues"
  },
  "dependencies": {
    "accept-language": "^2.0.17",
    "axios": "^0.13.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "bluebird": "^3.4.1",
